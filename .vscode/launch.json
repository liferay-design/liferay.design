{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Gatsby develop",
			"type": "node",
			"request": "launch",
			"protocol": "inspector",
			"program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
			"args": ["develop"],
			"stopOnEntry": false,
			"runtimeArgs": ["--nolazy"],
			"sourceMaps": false
		},
		{
			"name": "Gatsby build",
			"type": "node",
			"request": "launch",
			"protocol": "inspector",
			"program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
			"args": ["build"],
			"stopOnEntry": false,
			"runtimeArgs": ["--nolazy"],
			"sourceMaps": false
		}
	]
}
