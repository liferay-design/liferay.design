{
	"presets": [
		[
			"env",
			{
				"modules": false,
				"targets": {
					"browsers": ["last 3 versions"]
				}
			}
		],
		"react"
	],
	"plugins": ["transform-export-extensions", "transform-object-rest-spread"],
	"env": {
		"development": {
			"plugins": ["react-hot-loader/babel"]
		},
		"production": {
			"presets": ["react-optimize"]
		}
	}
}
